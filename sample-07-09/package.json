{
  "private": true,
  "name": "sample-fido2",
  "version": "0.0.1",
  "description": "Sample Frontend for FIDO 2",
  "main": "dist/index.js",
  "scripts": {
    "test": "yarn karma",
    "karma": "karma start",
    "karma:window": "yarn webpack && cross-env TEST_ENV=window karma start",
    "tsc": "tsc --build ./tsconfig.json",
    "webpack": "webpack --mode development --config webpack.config.js",
    "webpack:prod": "webpack --mode production --config webpack.config.js",
    "build": "rm -rf ./dist && yarn tsc && yarn webpack:prod",
    "cleanup": "rm -rf ./dist coverage ./node_modules"
  },
  "author": "Jun Kurihara",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/node": "14.14.25",
    "@types/webappsec-credential-management": "0.6.0",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "can-npm-publish": "1.3.3",
    "cross-env": "7.0.3",
    "eslint": "7.19.0",
    "jasmine-core": "3.6.0",
    "jest": "26.6.3",
    "karma": "6.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine": "4.0.1",
    "karma-typescript": "5.2.0",
    "ts-jest": "26.5.0",
    "ts-loader": "8.0.15",
    "typescript": "4.1.3",
    "webpack": "5.21.1",
    "webpack-cli": "4.5.0"
  },
  "dependencies": {
    "@fidm/x509": "1.2.1",
    "cbor-sync": "1.0.4",
    "js-crypto-utils": "1.0.0",
    "js-encoding-utils": "0.6.2"
  }
}
