{
  "private": true,
  "name": "sample-fido2",
  "version": "0.0.1",
  "description": "Sample Frontend for FIDO 2",
  "main": "dist/index.js",
  "scripts": {
    "test": "yarn karma",
    "karma": "karma start",
    "karma:window": "yarn webpack && cross-env TEST_ENV=window karma start",
    "tsc": "tsc --build ./tsconfig.json",
    "webpack": "webpack --mode development --config webpack.config.js",
    "webpack:prod": "webpack --mode production --config webpack.config.js",
    "build": "rm -rf ./dist && yarn tsc && yarn webpack:prod",
    "cleanup": "rm -rf ./dist coverage ./node_modules"
  },
  "author": "Jun Kurihara",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "16.11.28",
    "@types/webappsec-credential-management": "0.6.2",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "can-npm-publish": "1.3.6",
    "cross-env": "7.0.3",
    "eslint": "8.14.0",
    "jasmine-core": "4.1.0",
    "jest": "27.5.1",
    "karma": "6.3.19",
    "karma-chrome-launcher": "3.1.1",
    "karma-jasmine": "5.0.0",
    "karma-typescript": "5.5.3",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2"
  },
  "dependencies": {
    "@fidm/x509": "1.2.1",
    "cbor-sync": "1.0.4",
    "js-crypto-utils": "1.0.4",
    "js-encoding-utils": "0.7.1"
  }
}
